{
  "root": true,
  "plugins": [
    "@typescript-eslint",
    "prettier",
    "react"
  ],
  "parser": "@typescript-eslint/parser",
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": [
    "prettier"
  ],
  "rules": {
    "no-console": "error",
    "react/jsx-no-target-blank": "error",
    "prettier/prettier": "error"
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
